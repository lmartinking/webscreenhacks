extends ../layout

block extrahead
    title Dyslexic Typewriter

block extracss
    != css('typewriting')

block extrajs
    != js('keyboard_map')
    != js('typewriting')
    :coffeescript
        do ($ = jQuery) ->
            $ ->
                typewriter = new typing.TypeWriter $("#typewriting")
                typing.start_typwriter(typewriter)

block content
    #typewriting
